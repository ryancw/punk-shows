<li id="<%= feed_item.id %>">
    <span class="content"><%= feed_item.date %>
     - <%= feed_item.acts %> @ <%= feed_item.location %></span>
     <span class="content"><%= feed_item.details %></span>

  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method:  :delete,
                                     confirm: "You sure?",
                                     title:   feed_item.content %>
  <% end %>
</li>